ASPNETCORE_ENVIRONMENT=Staging
ASPNETCORE_URLS=http://0.0.0.0:8086
ConnectionStrings__Postgres=Host=localhost;Port=5432;Username=artifortress;Password=artifortress;Database=artifortress_staging
Auth__BootstrapToken=replace-with-staging-bootstrap-token
Auth__Oidc__Enabled=false
Auth__Oidc__Issuer=https://idp.staging.example.com
Auth__Oidc__Audience=artifortress-api
Auth__Oidc__Hs256SharedSecret=replace-with-staging-oidc-secret
Auth__Oidc__JwksJson=
Auth__Oidc__JwksUrl=
Auth__Oidc__JwksRefreshIntervalSeconds=300
Auth__Oidc__JwksRefreshTimeoutSeconds=10
Auth__Oidc__RoleMappings=groups|af-admins|*|admin
Auth__Saml__Enabled=false
Auth__Saml__IdpMetadataUrl=
Auth__Saml__ExpectedIssuer=
Auth__Saml__ServiceProviderEntityId=
Auth__Saml__RoleMappings=groups|af-admins|*|admin
Auth__Saml__IssuedPatTtlMinutes=60
ObjectStorage__Endpoint=http://localhost:9000
ObjectStorage__AccessKey=artifortress
ObjectStorage__SecretKey=artifortress
ObjectStorage__Bucket=artifortress-staging
